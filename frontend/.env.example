# Frontend environment variables
# Copy this file to .env and fill in real values

# Google OAuth 2.0 Client ID from Google Cloud -> Credentials
VITE_GOOGLE_CLIENT_ID=

# Backend API base URL (protocol + host, no trailing slash)
# Example: http://localhost:8080 or https://api.yourdomain.com
VITE_API_BASE_URL=

# Facebook App ID (optional if you use Facebook login)
VITE_FACEBOOK_APP_ID=
